ipaddress = "0.0.0.0"
port = 4646
hostname = "<%=@hostname%>"
log-file = "/opt/goiardi/log/goiardi.log"
syslog = false
log-level = "info"
index-file = "/opt/goiardi/data/goiardi-index.bin"
data-file = "/opt/goiardi/data/goiardi-data.bin"
freeze-interval = 600
use-unsafe-mem-store = false
time-slew = "100m"
conf-root = "/opt/goiardi/conf"
use-auth = true
use-ssl = false
https-urls = false
disable-webui = false
log-events = false
obj-max-size = 10485760
json-req-max-size = 1000000
max-connections = 50
use-serf = false
