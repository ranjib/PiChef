[tags]
[agent]
    interval = "10s"
    debug = false
    hostname = "<%=@hostname%>"
    utc = true
    precision = "s"
[outputs]
[outputs.influxdb]
    urls = ["<%=@url%>"]
    database = "<%=@database%>"
[cpu]
    percpu = true
    totalcpu = true
[net]
    interfaces = ["eth0"]
[ping]
    urls =["8.8.8.8"]
    count = 1
[disk]
[io]
[mem]
[system]
[swap]
[statsd]
  service_address = ":8125"
  delete_gauges = false
  delete_counters = false
  delete_sets = false
  delete_timings = true
  percentiles = [90]
  allowed_pending_messages = 10000
  percentile_limit = 1000
